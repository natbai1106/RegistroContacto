<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RegistroContacto.Informacion">
    <ContentPage.Content>
        <StackLayout Margin="10,10,10,290" HorizontalOptions="Center" HeightRequest="500" WidthRequest="500" >
            <Label x:Name="infnombre"
                   FontSize="Large"
                   Margin="20,75,20,20"
                   FontAttributes="Bold" />
            <Label x:Name="infedad"
                   FontSize="Large"
                   Margin="20,35,20,20"
                   FontAttributes="Bold"/>
            <Label x:Name="infcorreo"
                   FontSize="Large" 
                   Margin="20,35,20,20"
                   FontAttributes="Bold"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>